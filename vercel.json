{
  "routes": [
    { "src": "/", "dest": "/index.html" },
    { "src": "/audio/(.*)", "dest": "/audio/$1" },
    { "src": "/(.*)", "dest": "/$1" }
  ],
  "headers": [
    {
      "source": "/audio/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}